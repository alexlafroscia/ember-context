{
  "name": "@alexlafroscia/ember-context",
  "version": "1.0.1",
  "description": "Consume values from elsewhere in your Ember application",
  "keywords": [
    "ember-addon"
  ],
  "repository": "git://github.com/alexlafroscia/ember-context.git",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "dependencies": {
    "@glimmer/tracking": "^1.0.1",
    "ember-cli-babel": "^7.22.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.2",
    "@embroider/test-setup": "^0.47.0",
    "@glimmer/component": "^1.0.1",
    "@movable/eslint-config": "^0.6.7",
    "@movable/eslint-config-ember": "^1.0.1",
    "@movable/prettier-config": "^0.3.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.2.3",
    "ember-cli": "~3.28.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.7.0",
    "ember-template-lint-plugin-prettier": "^2.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.9.0",
    "eslint-plugin-ember": "^10.0.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.17.1",
    "qunit-dom": "^2.0.0",
    "standard-version": "^9.0.0",
    "webpack": "^5.58.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.hbs": "ember-template-lint --fix",
    "*.{html,json,md,yml}": "prettier --write"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write CHANGELOG.md"
    }
  },
  "volta": {
    "node": "12.18.3",
    "yarn": "1.22.5"
  }
}
